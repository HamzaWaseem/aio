<p id="notice"><%= notice %></p>

<h1>Car Sellings</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Rates</th>
      <th>Details</th>
      <th>Published at</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @car_sellings.each do |car_selling| %>
      <tr>
        <td><%= car_selling.title %></td>
        <td><%= car_selling.rates %></td>
        <td><%= car_selling.details %></td>
        <td><%= car_selling.published_at %></td>
        <td><%= link_to 'Show', car_selling %></td>
        <td><%= link_to 'Edit', edit_car_selling_path(car_selling) %></td>
        <td><%= link_to 'Destroy', car_selling, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Car Selling', new_car_selling_path %>
